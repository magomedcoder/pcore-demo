{
    "name": "pcore/pcore-demo",
    "description": "PCore demo",
    "type": "project",
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "pcore/http-server": "v1.2.*",
        "pcore/config": "v1.2.*",
        "pcore/aop": "v1.2.*",
        "pcore/di": "v1.2.*",
        "pcore/event": "v1.1.*",
        "pcore/session": "v1.2.*",
        "pcore/console": "v1.2.*",
        "pcore/database": "v1.2.*",
        "pcore/validator": "v1.1.*",
        "pcore/redis": "v1.2.*",
        "pcore/cache": "v1.2.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/Kernel/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
